  class DistanceCal{
	
	    //  private point p1;
	   //   private point p2;

		void   Adddist(point p1,point p2) {
	        double distance = Math.sqrt((p1.getX() - p2.getX()) *  (p1.getX() - p2.getX()) + 
	                                    (p1.getX() - p2.getX()) *  (p1.getX() - p2.getX()));
	        System.out.println(distance);

	
	  }
  }
  
  
  class main{
		
	  public static void main (String[] args) {
	      point p1 = new point(9.8,8.5);
	       point p2 = new point(7.8,8.9);
	      p1.Adddist();
	      p2.Adddist();

	
	  }
  }
	
	


	
	
